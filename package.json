{
  "name": "literacy",
  "description": "Literate programming in JavaScript using reStructuredText.",
  "version": "0.2.4",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/woofwoofinc/literacy",
  "repository": {
    "type": "git",
    "url": "https://github.com/woofwoofinc/literacy.git"
  },
  "bin": {
    "literacy": "./bin/literacy.js"
  },
  "scripts": {
    "build": "literacy src/js --out-dir lib && eslint lib"
  },
  "dependencies": {
    "detab": "^2.0.1",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "node-dir": "^0.1.17",
    "pegjs": "^0.10.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "eslint": "^4.6.0",
    "eslint-config-strict": "^14.0.0",
    "literacy": "^0.2.2"
  },
  "eslintConfig": {
    "extends": "strict",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "arrow-parens": ["error", "as-needed"],
      "id-blacklist": "off",
      "id-length": "off",
      "indent": "off",
      "no-console": "off",
      "no-magic-numbers": "off",
      "no-multiple-empty-lines": "off",
      "no-process-exit": "off",
      "no-sync": "off",
      "no-underscore-dangle": "off",
      "prefer-destructuring": "off",
      "padded-blocks": "off"
    }
  },
  "keywords": [
    "literate",
    "reStructuredText"
  ]
}
