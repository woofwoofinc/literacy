{
  "name": "literacy-loader",
  "description": "Webpack loader for Literacy .js.rst files.",
  "version": "1.1.0",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/woofwoofinc/literacy",
  "repository": {
    "type": "git",
    "url": "https://github.com/woofwoofinc/literacy.git"
  },
  "main": "lib/literacy-loader.js",
  "scripts": {
    "build": "literacy ../src/loader --out-dir lib && eslint ."
  },
  "dependencies": {
    "literacy": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.7.2",
    "eslint-config-strict": "^14.0.0"
  },
  "eslintConfig": {
    "extends": "strict",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "indent": "off"
    }
  },
  "keywords": [
    "webpack",
    "loader",
    "literacy",
    "literate",
    "reStructuredText"
  ]
}
